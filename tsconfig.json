{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2015",
    "lib": [
      "ES2015"
    ],
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "declaration": true,
    "newLine": "LF",
    "sourceMap": true,
    "outDir": "dist",
  },
  "exclude": [
    "node_modules",
    "dist",
    "dts",
    "test/actual",
    "test/expected",
    "test/fixture"
  ],
}
